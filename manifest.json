{
  "manifest_version": 2,

  "name": "Custom Playback Speed Manager",
  "description": "This extension will allow you to tweak the playback speed on a specific website",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": ["injected-script.js"]
}
